{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended"],
    "jsRules": {},
    "rules": {
      "arrow-parens": false,
      "trailing-comma": false,
      "semicolon": [true, "ignore-bound-class-methods"],
      "object-literal-sort-keys": false,
      "object-literal-key-quotes": false,
      "interface-name": false
